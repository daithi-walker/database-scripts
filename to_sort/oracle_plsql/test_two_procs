create or replace package dw_test
as
 procedure dw_proc1;
 procedure dw_proc2;
end dw_test;

create or replace package body dw_test
as
 procedure dw_proc1
 is
 begin
   DBMS_LOCK.SLEEP(20);
 end dw_proc1;
 procedure dw_proc2
 is
 begin
    dw_proc1;
    DBMS_LOCK.SLEEP(20);
 end dw_proc2;
end dw_test;

begin
  dw_test.dw_proc2;
end;